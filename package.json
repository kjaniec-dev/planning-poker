{
  "name": "planning-poker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:ws": "npm --prefix servers/node run dev",
    "dev:next": "next dev",
    "dev:external": "concurrently \"npm run dev:ws\" \"npm run dev:next\"",
    "dev:embedded": "NEXT_PUBLIC_REALTIME_URL= REALTIME_MODE=embedded NODE_ENV=production npm run build && REALTIME_MODE=embedded NODE_ENV=production node next-server.js",
    "build": "npm --prefix servers/node run build && next build",
    "start": "NODE_ENV=production node next-server.js",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tabs": "1.1.13",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "ioredis": "5.8.2",
    "lucide-react": "0.553.0",
    "next": "16.0.3",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@tailwindcss/postcss": "4.1.17",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "concurrently": "9.2.1",
    "nodemon": "3.1.11",
    "shadcn": "3.5.0",
    "tailwindcss": "4.1.17",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  }
}
